{
  "name": "todux-store",
  "version": "0.0.1",
  "description": "An implementation of a redux store intended for use in multiple todo applications.",
  "scripts": {
    "test": "mocha --compilers js:babel-register --recursive --reporter dot",
    "test:watch": "npm test -- --watch",
    "build": "babel -d dist src/",
    "build:watch": "npm run build -- --watch",
    "clean": "rm -rf dist/*",
    "prepublish": "npm run clean && npm test && npm run build"
  },
  "author": "Jonathan D. Johnson",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "expect": "^1.13.4",
    "mocha": "^2.4.5",
    "mockfirebase": "^0.12.0",
    "proxyquire": "^1.7.4",
    "redux-mock-store": "0.0.6"
  },
  "dependencies": {
    "firebase": "^2.4.0",
    "lodash": "^4.2.1",
    "redux": "^3.1.2",
    "redux-thunk": "^1.0.3",
    "shortid": "^2.2.4"
  }
}
